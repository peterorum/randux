{
  "name": "randux",
  "version": "1.0.0",
  "description": "Display a random word using react+redux.",
  "main": "index.js",
  "scripts": {
    "serve": "webpack-dev-server --inline --hot --progress --colors",
    "start": "NODE_ENV=development npm run images && webpack && npm run serve",
    "images": "npm run imagemin && npm run svgo",
    "imagemin": "imagemin ../design/images/**/* --out-dir=images",
    "svgo": "svgo --input=../design/svg --output=images/icons --config=./.svgo.yml",
    "test": "NODE_ENV=production mocha --recursive './source/**/*.spec.js' --compilers js:babel-core/register",
    "test:watch": "npm run test -- --watch",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run images && npm run build:webpack",
    "clean": "rimraf dist/*",
    "webpack": "NODE_ENV=development webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peterorum/randux"
  },
  "author": "peter@peterorum.com",
  "license": "MIT",
  "devDependencies": {
    "animate.css": "^3.5.2",
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "critical": "^0.8.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "imagemin-cli": "^3.0.0",
    "immutable": "^3.8.1",
    "jsx-chai": "^4.0.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.10.1",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.1.1",
    "postcss-pxtorem": "^3.3.1",
    "raven-js": "^3.8.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "svgo": "^0.7.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-svgstore-plugin": "^3.0.6"
  },
  "dependencies": {}
}
