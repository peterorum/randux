{
  "name": "randux",
  "version": "1.0.0",
  "description": "Display a random word using react+redux.",
  "main": "index.js",
  "scripts": {
    "serve": "webpack-dev-server --inline --hot --progress --colors",
    "start": "npm run images && webpack && npm run serve",
    "images": "npm run imagemin && npm run svgo",
    "imagemin": "imagemin ../design/images/**/* --out-dir=images",
    "svgo": "svgo --input=../design/icons --output=images/icons --config=./.svgo.yml",
    "test": "mocha './tests/**/*.spec.js' --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
