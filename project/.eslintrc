{
  "extends": "eslint:recommended",
  "plugins": [],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
  "_": false
  },
  "rules": {
  "no-mixed-spaces-and-tabs": 0,
  "comma-dangle": 0,
  "complexity": [2, 7],
  "max-depth": [2, 4],
  "max-statements": [2, {"maximum": 20}, {"ignoreTopLevelFunctions": false}],
  "no-console": 0,
  "no-extra-semi": 0,
  "no-constant-condition": 1
  }
}
